	openEHR-EHR-OBSERVATION.webtemplate_datatypes.v1

concept
	[at0000]	-- Webtemplate datatypes
language
	original_language = <[ISO_639-1::de]>
description
	original_author = <
		["name"] = <"">
	>
	details = <
		["de"] = <
			language = <[ISO_639-1::de]>
			purpose = <"Archetype der alle Datentypen verwendet, um Pfadextraktion aus WebTemplates für alle verschiedenen Datentypen zu testen.">
			use = <"">
			misuse = <"">
			copyright = <"">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
	other_details = <
		["MD5-CAM-1.0.1"] = <"23EAECEA6EF43C4CD5767B664D60AC3A">
	>

definition
	OBSERVATION[at0000] matches {	-- Webtemplate datatypes
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Jedes Ereignis
						data matches {
							ITEM_TREE[at0003] matches {	-- Baum
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- Cluster111
										items cardinality matches {2..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {	-- Text-Element-Free
												value matches {
													DV_TEXT matches {*}
												}
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {[openehr::271]}
													}
												}
											}
											ELEMENT[at0011] occurrences matches {0..1} matches {	-- Text-Element-Coded-Intern
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0012, 	-- Begriff1
															at0013, 	-- Begriff2
															at0014]	-- Begriff3
														}
													}
												}
											}
											ELEMENT[at0015] occurrences matches {0..1} matches {	-- Text-Element-Coded-Term
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {[ac0001]}		-- Neue Einschränkung
													}
												}
											}
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- Quantity-Element
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::129]>
														list = <
															["1"] = <
																units = <"m3">
															>
														>
													>
												}
											}
											ELEMENT[at0007] occurrences matches {0..1} matches {	-- Amount-Element
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..10|}
													}
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- DateAndTime-Element
												value matches {
													DV_DATE_TIME matches {
														value matches {yyyy-mm-ddTHH:MM:SS}
													}
												}
											}
											ELEMENT[at0024] occurrences matches {0..1} matches {	-- Date-Element
												value matches {
													DV_DATE matches {
														value matches {yyyy-mm-dd}
													}
												}
											}
											ELEMENT[at0025] occurrences matches {0..1} matches {	-- Time-Element
												value matches {
													DV_TIME matches {
														value matches {HH:MM:SS}
													}
												}
											}
											ELEMENT[at0009] occurrences matches {0..1} matches {	-- Duration-Element
												value matches {
													DV_DURATION matches {
														value matches {|P0D..P10D|}
													}
												}
											}
											ELEMENT[at0010] matches {	-- Ordinal-Element
												value matches {
													1|[local::at0021], 	-- Orindalwert1
													2|[local::at0022], 	-- Ordinalwert2
													3|[local::at0023]  	-- Ordinalwert3
												}
											}
											ELEMENT[at0016] matches {	-- Bool-Element
												value matches {
													DV_BOOLEAN matches {
														value matches {True, False; False}
													}
												}
												null_flavour existence matches {0..1} matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[openehr::
															271, 
															272, 
															273]
														}
													}
												}
											}
											ELEMENT[at0027] occurrences matches {0..1} matches {	-- Integerintervall
												value matches {
													DV_INTERVAL<DV_COUNT> matches {
														upper matches {
															DV_COUNT matches {
																magnitude matches {|<=10|}
															}
														}
														lower matches {
															DV_COUNT matches {
																magnitude matches {|>=0|}
															}
														}
													}
												}
											}
											ELEMENT[at0028] occurrences matches {0..1} matches {	-- Quantity-intervall
												value matches {
													DV_INTERVAL<DV_QUANTITY> matches {
														upper matches {
															C_DV_QUANTITY <
																property = <[openehr::129]>
																list = <
																	["1"] = <
																		units = <"m3">
																	>
																>
															>
														}
														lower matches {
															C_DV_QUANTITY <
																property = <[openehr::129]>
																list = <
																	["1"] = <
																		units = <"m3">
																	>
																>
															>
														}
													}
												}
											}
											ELEMENT[at0029] occurrences matches {0..1} matches {	-- DateAndTime-Intervall
												value matches {
													DV_INTERVAL<DV_DATE_TIME> matches {
														upper matches {
															DV_DATE_TIME matches {
																value matches {yyyy-mm-ddTHH:MM:SS}
															}
														}
														lower matches {
															DV_DATE_TIME matches {
																value matches {yyyy-mm-ddTHH:MM:SS}
															}
														}
													}
												}
											}
											ELEMENT[at0030] occurrences matches {0..1} matches {*}
											ELEMENT[at0031] occurrences matches {0..1} matches {	-- Multiple-Element
												value matches {
													DV_MULTIMEDIA matches {
														media_type matches {[openEHR::427]}
													}
													DV_URI matches {*}
												}
											}
											ELEMENT[at0032] occurrences matches {0..1} matches {	-- Multimedia-JPEG-Element
												value matches {
													DV_MULTIMEDIA matches {
														media_type matches {[openEHR::429]}
													}
												}
											}
											ELEMENT[at0033] occurrences matches {0..1} matches {	-- URI-Element
												value matches {
													DV_URI matches {*}
												}
											}
											ELEMENT[at0034] occurrences matches {0..1} matches {	-- Proportion-percent-Element
												value matches {
													DV_PROPORTION matches {
														is_integral matches {True}
													}
												}
											}
											ELEMENT[at0035] occurrences matches {0..1} matches {	-- Proportion-Fraction-Element
												value matches {
													DV_PROPORTION matches {
														numerator matches {|0.0..10.0|}
														denominator matches {|0.0..10.0|}
														is_integral matches {False}
													}
												}
											}
											ELEMENT[at0036] occurrences matches {0..1} matches {	-- Identifier-Element
												value matches {
													DV_IDENTIFIER matches {
														issuer matches {/Issuer/}
													}
												}
											}
											ELEMENT[at0039] occurrences matches {0..1} matches {	-- Parsbar-Element
												value matches {
													DV_PARSABLE matches {
														formalism matches {"text/plain", "text/html"}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

